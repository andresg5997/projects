{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "anhskohbo/no-captcha": "^2.3",
        "barryvdh/laravel-debugbar": "^2.3",
        "chencha/share": "^5.2",
        "devdojo/chatter": "0.2.*",
        "doctrine/dbal": "^2.5",
        "froiden/laravel-installer": "^1.1",
        "fx3costa/laravelchartjs": "^2.2",
        "graham-campbell/markdown": "^7.1",
        "guzzlehttp/guzzle": "^6.2",
        "intervention/image": "^2.3",
        "james-heinrich/getid3": "^1.9",
        "jenssegers/agent": "^2.4",
        "jpmurray/laravel-rrule": "^3.2",
        "jrean/laravel-user-verification": "^4.1",
        "laracasts/flash": "^2.0",
        "laravel/cashier-braintree": "~2.0",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.*",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "luketowers/purifier": "~3.0",
        "maatwebsite/excel": "~2.1.0",
        "maddhatter/laravel-fullcalendar": "^1.2",
        "mpociot/teamwork": "~5.0",
        "nikic/php-parser": "^3.0",
        "phpoffice/phpexcel": "1.8.1",
        "pion/laravel-chunk-upload": "^1.0",
        "predis/predis": "^1.1",
        "rtconner/laravel-likeable": "~1.2",
        "rtconner/laravel-tagging": "~2.2",
        "spatie/calendar-links": "^1.0",
        "spatie/flysystem-dropbox": "^1.0",
        "spatie/laravel-backup": "^4.13",
        "torann/geoip": "^1.0",
        "yajra/laravel-datatables-oracle": "~6.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.3",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
